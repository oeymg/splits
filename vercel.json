{
  "version": 2,
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/split/(.*)",
      "dest": "/split.html"
    },
    {
      "src": "/pricing",
      "dest": "/pricing.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
